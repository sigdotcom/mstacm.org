{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-microsoft-contrib",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "missing-jsdoc": false,
        "no-relative-imports": false,
        "jsx-wrap-multiline": false,
        "export-name": false,
        "import-name": false,
        "variable-name": {
          "options": [
            "ban-keywords",
            "check-format",
            "allow-pascal-case"
          ]
        },
        "max-func-body-length": false
    },
    "rulesDirectory": []
}
